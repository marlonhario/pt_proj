    "scripts": {
        "server-dev": "nodemon --watch ./server/src/ -e ts,js,json -r esm --exec ts-node --project ./config/typescript/server.json ./server/src/index.ts",
        "front-dev": "react-app-rewired start --config-overrides ./config/react-compile.cjs",
        "dev": "concurrently \"npm run server-dev\" \"npm run front-dev\" ",
        "build-server": "tsc --project ./config/typescript/server.json",
        "build-front": "react-app-rewired build --config-overrides ./config/react-compile.js",
        "build": "npm run build-server && npm run build-front",
        "server-prod": "ts-node --project ./config/typescript/server.json ./dist/server/index.js",
        "front-prod": "ts-node ./index.ts",
        "prod": "npm run build && concurrently \"npm run server-prod\" \"npm run front-prod\" ",
        "front-codegen": "graphql-codegen --config ./config/graphql/codegen.yml"
    },
